{
    "name": "olssonm/laravel-backup-shield",
    "description": "Protection for your laravel backups",
    "keywords": [
        "olssonm",
        "Laravel 5",
        "backup",
        "password",
        "security"
    ],
    "homepage": "https://github.com/olssonm/laravel-backup-shield",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "nelexa/zip": "3.3",
        "spatie/laravel-backup": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\BackupShield\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\BackupShield\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Olssonm\\BackupShield\\BackupShieldServiceProvider"
            ]
        }
    },
    "sort-packages": true,
    "minimum-stability": "stable"
}
