{
    "name": "olssonm/laravel-backup-shield",
    "description": "Protection for your laravel backups",
    "keywords": [
        "olssonm",
        "Laravel 5",
        "backup",
        "password",
        "security"
    ],
    "homepage": "https://github.com/olssonm/laravel-backup-shield",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "illuminate/support": "~5.5.0|~5.6.0",
        "php" : "^7.0",
        "nelexa/zip": "^3.1",
        "spatie/laravel-backup": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\BackupShield\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\BackupShield\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Olssonm\\BackupShield\\BackupShieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
